{
    "name": "modelical/yii2-app-advanced",
    "description": "Yii 2 Advanced Project Template with REST API",
    "keywords": ["yii2", "framework", "advanced", "project template", "rest", "api"],
    "homepage": "http://www.modelical.com/yii-template",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/modelical/yii2-app-advanced/issues",
